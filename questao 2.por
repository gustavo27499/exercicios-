programa {
  funcao inicio() {
    cadeia caminho
    escreva("escolha um caminho(esquerda ou direita): ")
    leia(caminho)
    se (caminho =="esquerda")
    escreva("voce encontrou um tesouro!")
    senao se (caminho == "direita")
    escreva("voce caiu em uma armadilha!")
    senao
    escreva("escolha invalida, tente novamente!")
    
  }
}
